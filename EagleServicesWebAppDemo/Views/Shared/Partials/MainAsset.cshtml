@using System.Configuration;
<!-- begin::Head -->
<head>
    <meta charset="utf-8" />
    <link rel='shortcut icon' type='image/x-icon'/>
    @*<link rel='shortcut icon' type='image/x-icon' href='~/favicon.ico' />*@
    @*<link rel="icon" href="./favicon.ico" type="image/x-icon" />
        <link rel="shortcut icon" href="./favicon.ico" type="image/x-icon" />*@
    <link rel='shortcut icon' type='image/x-icon' href="@Url.Content(ConfigurationManager.AppSettings.Get("MainHeaderLogoPath"))" />
    <title>
        @ViewBag.AppsName  | @ViewBag.Title
    </title>
    <meta name="description" content="Bootstrap notify examples">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Poppins:300,400,500,600,700", "Roboto:300,400,500,600,700"] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <style>
        .required {
            display: inline-block;
            color: #d2322d;
            font-size: 0.8em;
            font-weight: bold;
            position: relative;
            top: -0.2em;
        }
    </style>
    <!--end::Web font -->
    <!--begin::Base Styles -->
    <link href="@Url.Content("~/Content/assets/vendors/base/vendors.bundle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/assets/demo/default/base/style.bundle.css")" rel="stylesheet" type="text/css" />
    <!--end::Base Styles -->
    <!--begin::Kendo Style -->
    <link href="@Url.Content("~/Content/kendo/2017.3.1018/kendo.common-material.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/kendo/2017.3.1018/kendo.rtl.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/kendo/2017.3.1018/kendo.material.mobile.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/kendo/2017.3.1018/kendo.material.min.css")" rel="stylesheet" type="text/css" />
    <!--end::Kendo Style -->

    <link rel="shortcut icon" href="@Url.Content("~/Content/assets/demo/default/media/img/logo/logos.ico")" />
    <style>
        .page-number.clickable.active {
            background: #4286f4 none repeat scroll 0 0;
            border-radius: 5px;
            color: white;
            margin: 2px;
            padding: 10px;
        }

        .page-number.clickable {
            background: lightgrey none repeat scroll 0 0;
            border-radius: 5px;
            margin: 1px;
            padding: 10px;
            cursor: pointer;
        }

        .new {
            margin-top: -5px;
            position: absolute;
            right: 40px;
            transform: rotate(35deg);
        }

        span.active {
            background: white none repeat scroll 0 0;
            border-radius: 5px;
            color: #777;
            padding: 5px;
        }

        span.doc {
            cursor: pointer;
        }

        .k-button.k-button-icontext.k-grid-delete span.k-icon.k-i-close:before {
            content: "\e10c"
        }
    </style>
    <!--begin::Base Scripts -->
    <script src="@Url.Content("~/Content/assets/vendors/base/vendors.bundle.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/kendo/2017.3.1018/jszip.min.js")"></script>
    <!--end::Base Scripts -->
    <!--Reference the SignalR library. -->
    <script src="@Url.Content("~/Scripts/jquery.signalR-2.2.2.min.js")"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="@Url.Content("~/signalr/hubs")"></script>

    <!--begin::Kendo Scripts -->
    <script src="@Url.Content("~/Scripts/kendo/2017.3.1018/kendo.all.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo/2017.3.1018/kendo.aspnetmvc.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo/2017.3.1018/cultures/kendo.culture." + @ViewBag.Culture + ".min.js")"></script>
    <!--end::Kendo Scripts -->

    <script>
        kendo.culture("@ViewBag.Culture");

        function current_url() {
            var current_url = "@Request.Url.ToString()";
            return current_url;
        }

        function setNotification(title, msg, type) {
            var content = {};
            content.title = title;
            content.message = msg;
            $.notify(content, {
                type: type,
                allow_dismiss: false,
                newest_on_top: false,
                mouse_over: false,
                showProgressbar: false,
                spacing: '10',
                timer: '2000',
                placement: {
                    from: 'top',
                    align: 'right'
                },
                offset: {
                    x: '30',
                    y: '30'
                },
                delay: '1000',
                z_index: '10000',
                animate: {
                    enter: 'animated bounce',
                    exit: 'animated bounce'
                }
            });
        }
    </script>
</head>
<!-- end::Head -->